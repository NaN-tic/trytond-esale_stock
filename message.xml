<tryton>
    <data group="1">
      <record model="ir.message" id="stock_not_export">
          <field name="text">There are not stock to export</field>
      </record>
      <record model="ir.message" id="select_date_stocks">
          <field name="text">Select a date to export stocks</field>
      </record>
