<form>
    <image name="tryton-info" xexpand="0"
        xfill="0"/>
    <group col="1" id="labels">
        <label
            string="Export eSale stock to CSV file."
            id="create"
            yalign="0.0" xalign="0.0" xexpand="1"/>
        <label string="Note that this operation may take a few minutes."
            id="operation"
            yalign="0.0" xalign="0.0" xexpand="1"/>
    </group>
    <group col="4" id="fields">
        <label name="shop"/>
        <field name="shop" widget="selection"/>
        <label name="from_date"/>
        <field name="from_date"/>
    </group>
</form>
