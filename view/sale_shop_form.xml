<data>
    <xpath
        expr="/form/notebook/page[@id='esale']/notebook/page[@id='catalog']/field[@name='esale_last_prices']"
        position="after">
            <newline/>
            <button name="export_stocks" string="Export Stock"/>
            <label name="esale_last_stocks"/>
            <field name="esale_last_stocks"/><newline/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='esale']/notebook/page[@id='configuration']/notebook/page[@id='general']/field[@name='esale_timezone']"
        position="after">
            <label name="esale_forecast_quantity"/>
            <field name="esale_forecast_quantity"/><newline/>
    </xpath>
</data>
